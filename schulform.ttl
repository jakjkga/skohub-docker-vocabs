@prefix schulform: <https://example.org/schulform/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

schulform: a skos:ConceptScheme ;
    dct:title "schulform"@de ;
    dct:creator "Joachim Jakob"@de ;
    dct:created "2021-11-02"^^xsd:date ;
    dct:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
    skos:hasTopConcept schulform:AS, schulform:BES, schulform:BB .

schulform:AS a skos:Concept ;
    skos:prefLabel "Allgemeinbildende Schule"@de ;
    skos:altLabel "Allg. Schule"@de ;
    skos:narrower schulform:GS, schulform:MS, schulform:RS, schulform:GYM ;
    skos:topConceptOf schulform: .

schulform:GS a skos:Concept ;
    skos:prefLabel "Grundschule"@de ;
    skos:altLabel "Grundsch."@de ;
    skos:broader schulform:AS ;
    skos:inScheme schulform: .

schulform:MS a skos:Concept ;
    skos:prefLabel "Mittelschule"@de ;
    skos:altLabel "Mittelsch."@de ;
    skos:altLabel "Hauptschule"@de ;
    skos:broader schulform:AS ;
    skos:inScheme schulform: .

schulform:RS a skos:Concept ;
    skos:prefLabel "Realschule"@de ;
    skos:altLabel "Realsch."@de ;
    skos:broader schulform:AS ;
    skos:inScheme schulform: .

schulform:GYM a skos:Concept ;
    skos:prefLabel "Gymnasium"@de ;
    skos:altLabel "Gymn."@de ;
    skos:broader schulform:AS ;
    skos:inScheme schulform: .

schulform:BES a skos:Concept ;
    skos:prefLabel "Berufliche Schule"@de ;
    skos:altLabel "Berufl. Schule"@de ;
    skos:narrower schulform:BS, schulform:BFS, schulform:FAK, schulform:FS ;
    skos:topConceptOf schulform: .

schulform:BS a skos:Concept ;
