@prefix schulbereich: <https://example.org/schulbereich/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

schulbereich: a skos:ConceptScheme ;
    dct:title "Schulbereich"@de ;
    dct:creator "Joachim Jakob"@de ;
    dct:created "2021-11-02"^^xsd:date ;
    dct:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
    skos:hasTopConcept schulbereich:AS, schulbereich:BES, schulbereich:BB .

schulbereich:AS a skos:Concept ;
    skos:prefLabel "Allgemeinbildende Schule"@de ;
    skos:altLabel "Allg. Schule"@de ;
    skos:topConceptOf schulbereich: .

schulbereich:BES a skos:Concept ;
    skos:prefLabel "Berufliche Schule"@de ;
    skos:altLabel "Berufl. Schule"@de ;
    skos:narrower schulbereich:BS, schulbereich:BFS, schulbereich:FAK, schulbereich:FS ;
    skos:topConceptOf schulbereich: .

schulbereich:BB a skos:Concept ;
    skos:prefLabel "Berufsbegleitend"@de ;
    skos:altLabel "Berufsbegl."@de ;
    skos:topConceptOf schulbereich: .

schulbereich:BS a skos:Concept ;
    skos:prefLabel "Berufsschule"@de ;
    skos:altLabel "Berufssch."@de ;
    skos:broader schulbereich:BES ;
    skos:topConceptOf schulbereich: .

schulbereich:BFS a skos:Concept ;
    skos:prefLabel "Berufsfachschule"@de ;
    skos:altLabel "Berufsfachsch."@de ;
    skos:broader schulbereich:BES ;
    skos:topConceptOf schulbereich: .

schulbereich:FAK a skos:Concept ;
    skos:prefLabel "Fachakademie"@de ;
    skos:altLabel "Fachakad."@de ;
    skos:broader schulbereich:BES ;
    skos:topConceptOf schulbereich: .

schulbereich:FS a skos:Concept ;
    skos:prefLabel "Fachschule"@de ;
    skos:altLabel "Fachsch."@de ;
    skos:broader schulbereich:BES ;
    skos:topConceptOf schulbereich: .

